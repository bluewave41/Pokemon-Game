extends layout

block content
  h1= "Register"
  form(action='/users/create', method='POST')
    label= 'Username'
    br
    input(
      type='text'
      name='username'
    )
    br
    label= 'Password'
    br
    input(
      type='text'
      name='password'
    )
    br
    input(
      type='submit'
      value='Submit'
    )
